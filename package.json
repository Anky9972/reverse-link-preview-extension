{
  "name": "reverse-link-preview",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:main && npm run build:content && npm run copy:assets",
    "build:main": "vite build",
    "build:content": "vite build --config vite.content.config.js",
    "copy:assets": "powershell -Command \"Copy-Item -Path 'public\\*' -Destination 'dist' -Recurse -Force\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:extension": "npm run build"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.13",
    "lucide-react": "^0.544.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0",
    "tailwindcss": "^4.0.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "vite": "^6.2.0"
  }
}
